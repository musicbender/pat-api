    "@types/koa-router": "^7.0.42",
    "@types/node": "^12.0.10",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.3",
    "babel-jest": "^24.8.0",
    "concurrently": "^4.1.1",
    "graphql-tag": "^2.0.0",
    "graphql-tag-pluck": "^0.8.3",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.0.2",
