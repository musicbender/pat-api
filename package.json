{
  "name": "pat-api",
  "version": "1.5.5",
  "description": "My life tracking GraphQL API service",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "dev:d": "docker-compose up",
    "dev": "nodemon",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:prod": "export NODE_ENV=production; ts-node node_modules/.bin/sequelize db:migrate",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "tdd": "jest --watch",
    "test": "jest",
    "generate": "graphql-codegen -r dotenv/config",
    "postversion": "sh .deployment/scripts/version.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musicbender/pat-api.git"
  },
  "keywords": [],
