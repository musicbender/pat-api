{
  "name": "pat-api",
  "version": "1.6.0",
  "description": "My life tracking GraphQL API service",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "docker-compose up",
    "start:nodemon": "nodemon",
    "db:migrate": "ts-node node_modules/.bin/sequelize db:migrate",
    "db:migrate:prod": "export NODE_ENV=production; ts-node node_modules/.bin/sequelize db:migrate",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "tdd": "jest --group=unit --watch",
    "test": "jest --group=unit",
    "test:int": "jest --group=integration --setupFiles ./test/integration/setup.ts",
    "generate": "graphql-codegen -r dotenv/config",
    "postversion": "sh .deployment/scripts/version.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/musicbender/pat-api.git"
  },
  "keywords": [],
  "author": "Pat Jacobs",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/musicbender/pat-api/issues"
  },
  "homepage": "https://github.com/musicbender/pat-api#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-document-nodes": "1.17.10",
    "@graphql-codegen/typescript-mongodb": "^1.18.1",
    "@graphql-codegen/typescript-resolvers": "1.19.0",
    "@types/bluebird": "^3.5.28",
    "@types/dotenv": "^8.2.0",
    "@types/graphql": "^14.2.2",
    "@types/jest": "^26.0.22",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-helmet": "^6.0.2",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^14.14.37",
    "@types/sequelize": "^4.28.4",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.1.3",
    "@types/winston": "^2.4.4",
    "babel-jest": "^26.6.3",
    "concurrently": "^6.0.1",
    "graphql-tag": "^2.0.0",
    "graphql-tag-pluck": "^0.8.3",
    "jest": "^26.6.3",
    "jest-runner-groups": "^2.2.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.2.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "altair-koa-middleware": "^4.0.0",
    "aws-sdk": "^2.507.0",
    "axios": "^0.21.1",
    "chalk": "^4.1.0",
    "dotenv": "^8.0.0",
    "graphql": "^15.5.0",
    "graphql-date": "^1.0.3",
    "graphql-middleware": "^6.0.7",
    "graphql-union-input-type": "^0.4.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-combine-routers": "^4.0.2",
    "koa-graphql": "^0.8.0",
    "koa-helmet": "^6.1.0",
    "koa-jwt": "^4.0.1",
